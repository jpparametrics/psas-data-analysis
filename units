#!/usr/bin/awk -f

$3 == "GPS_1000" {
	printf "%s 1000 %d/%02d/%02d %02d:%02d:%02d %.8f %.8f %.2f\n", $1,
		$16, $14, $15, $17, $18, $19,
		$21/314159265.35*180, $22/314159265.35*180, $23/100
}

$3 == "GPS_1009" {
	printf "%s 1009 %f %.2f %.2f %.2f %.2f %.2f %.2f\n", $1,
		sqrt($10**2+$11**2+$12**2)/100,
		$10/100, $11/100, $12/100,
		$7/100, $8/100, $9/100
}
